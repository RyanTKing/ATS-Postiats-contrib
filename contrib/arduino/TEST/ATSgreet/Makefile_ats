#
MAKE=make
PATSOPT=$(PATSHOME)/bin/patsopt
#
all:: \
ATSgreet_dats.ino
ATSgreet_dats.ino: \
ATSgreet.dats; $(PATSOPT) -o $@ --dynamic $<
#
all:: ; $(MAKE) -f Makefile
#
RMF=rm -f
#
######
#
clean:: ; $(RMF) *~
clean:: ; $(RMF) *_?ats.c
#
######
#
cleanall:: clean
cleanall:: ; $(RMF) *_dats.ino
cleanall:: ; $(MAKE) -f Makefile clean
#
###### end of [Makefile] ######
